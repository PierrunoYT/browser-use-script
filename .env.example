# OpenAI (Default)
OPENAI_API_KEY=your_openai_api_key_here
BROWSER_USE_API_KEY=your_browser_use_api_key_here  # Optional

# LLM Provider Selection (openai, anthropic, or azure)
LLM_PROVIDER=openai

# System Prompt Selection (default, safety, or collection)
SYSTEM_PROMPT=default

# Anthropic (Optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Azure OpenAI (Optional)
AZURE_OPENAI_ENDPOINT=https://your-endpoint.openai.azure.com/
AZURE_OPENAI_KEY=your_azure_openai_key_here

# Telemetry Settings
# Set to false to opt out of anonymous usage data collection
ANONYMIZED_TELEMETRY=true
